        <documentation>
            Placeholder
        </documentation>
    </arg-definition>
    <arg-definition name="role" type="string">
        <documentation>
            Placeholder
        </documentation>
    </arg-definition>
    <arg-definition name="output" type="file">
        <documentation>
            The file that you would like to have as output.
        </documentation>
    </arg-definition>

    <arg-definition name="key" type="string">
        <documentation>
            Placeholder
        </documentation>
    </arg-definition>

